{
  "name": "v-server",
  "version": "0.0.1",
  "private": true,
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-0": "6.5.0",
    "co-body": "^4.2.0",
    "co-request": "^1.0.0",
    "co-views": "^2.1.0",
    "express": "^4.18.1",
    "fs": "0.0.2",
    "koa": "2.13.4",
    "koa-basic-auth": "4.0.0",
    "koa-logger": "3.2.1",
    "koa-membership": "0.0.3",
    "koa-mount": "4.0.0",
    "koa-router": "11.0.1",
    "koa-static": "5.0.0",
    "koa-to-express": "git+https://github.com/jeff-tian/koa-to-express.git",
    "socket.io": "^2.4.0",
    "sql.js": "^0.3.2",
    "uuid": "^3.1.0"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "V_ADMIN=test V_PWD=test babel-node app.js",
    "client": "PORT=80 babel-node start-client.js",
    "test": "V_ADMIN=test V_PWD=test mocha --harmony&&cd client&&npm test&&cd .. ",
    "e2e-test": "concurrently \"yarn e2e-driver\" \"yarn start\" \"sleep 10 && yarn e2e\"",
    "server-test": "V_ADMIN=test V_PWD=test mocha --harmony",
    "client-test": "cd client && npm run test-all && cd ..",
    "dev": "echo \"This command has been deprecated. Use 'npm start'\" && exit 1",
    "build": "cd client && npm run build && cd .. && cp vercel.json client/build/vercel.json",
    "prd": "NODE_ENV=production babel-node app.js",
    "e2e": "protractor ./tests/config.js",
    "e2e-driver": "webdriver-manager update && webdriver-manager start",
    "okteto": "k8ss switch --cluster=okteto --namespace=jeff-tian&&kubectl apply -k k8s&&kubectl rollout restart deploy v",
    "serverless": "V_ADMIN=test V_PWD=test node api/v.js"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "co-mocha": "^1.1.3",
    "co-supertest": "0.0.10",
    "concurrently": "^3.1.0",
    "expect": "^21.1.0",
    "expect.js": "^0.3.1",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.5.3",
    "protractor": "^5.1.2",
    "selenium-webdriver": "^3.5.0",
    "supertest": "^2.0.1",
    "webpack": "^3.6.0"
  }
}
