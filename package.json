{
  "name": "lookup-server",
  "version": "0.0.1",
  "private": true,
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-0": "6.5.0",
    "classnames": "^2.2.5",
    "co-body": "^4.2.0",
    "co-request": "^1.0.0",
    "co-views": "^2.1.0",
    "express": "4.13.3",
    "fs": "0.0.2",
    "jquery": "^3.2.1",
    "koa": "^1.2.4",
    "koa-basic-auth": "^2.0.0",
    "koa-logger": "^1.3.0",
